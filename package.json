{
  "name": "aurelia-typescript-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --hot --open",
    "build": "webpack --progress --hide-modules",
    "test": "karma start test/karma.conf.js --singleRun",
    "test:watch": "karma start test/karma.conf.js",
    "e2e": "npm run e2e:install && npm run e2e:run",
    "e2e:install": "webdriver-manager update",
    "e2e:run": "protractor test/protractor.conf.js",
    "e2e:debug": "protractor test/protractor.conf.js --elementExplorer",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "ag-grid": "^8.2.0",
    "ag-grid-aurelia":"^8.2.0",
    "aurelia-bootstrapper": "2.1.0",
    "aurelia-dialog": "^1.0.0-beta.3.0.1",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-framework": "1.0.8",
    "aurelia-history-browser": "1.0.0",
    "aurelia-i18n": "^1.3.1",
    "aurelia-loader-webpack": "^2.1.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-materialize-bridge": "^0.20.4",
    "aurelia-templating-binding": "1.2.0",
    "aurelia-templating-resources": "1.2.0",
    "aurelia-templating-router": "1.0.1",
    "bundle-loader": "^0.5.5",
    "i18next-xhr-backend": "^1.3.0",
    "materialize-css": "^0.97.7"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.41",
    "@types/webpack": "2.2.2",
    "@types/webpack-env": "1.13.0",
    "aurelia-core-dependencies": "https://github.com/jods4/aurelia-webpack-build.git#aurelia-core-deps",
    "aurelia-protractor-plugin": "https://github.com/aurelia/protractor-plugin",
    "aurelia-testing": "1.0.0-beta.2.0.1",
    "aurelia-webpack-plugin": "2.0.0-rc.1",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "css-loader": "^0.26.2",
    "file-loader": "^0.10.1",
    "html-loader": "0.4.5",
    "jasmine-core": "2.5.2",
    "json-loader": "^0.5.4",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.2",
    "protractor": "5.0.0",
    "style-loader": "^0.13.2",
    "ts-node": "2.0.0",
    "typescript": "2.2.x",
    "url-loader": "^0.5.8",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  },
  "peerDependencies": {}
}
